
@font-face{
  font-family: 'Poppins', sans-serif;
  src: url('https://fonts.googleapis.com/css2?family=Poppins:wght@100&display=swap');
}
.label{
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
    width: 740rpx;
    height:130rpx;
    padding: 40rpx;
    margin:10rpx;
    color: white;
    font-weight: 500;
    background-image: linear-gradient(130deg, #f7bd00c2 0%, #fcf40093 85%, #f8cf00 100%);
    border-radius: 30px;
}
.msg{
  width: 740rpx;
  height:50rpx;
margin-top:-5px;
padding-bottom: 3px;
font-weight: bolder;

}
.date{
  width: 740rpx;
  height:50rpx;
  margin-top: -6px;
}

